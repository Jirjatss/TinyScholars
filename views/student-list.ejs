<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Daftar Siswa</title>
</head>
<body>
    <h1>Daftar Siswa</h1>
    <table border="1">
        <thead>
          <tr>
            <th>No.</th>
            <th>Foto</th>
            <th>Nama Lengkap</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
          <% students.forEach((student, index) => { %>
            <tr>
              <td><%= index + 1 %></td>
              <td><img src="<%= student.UserProfile.imageUrl %>" alt="<%= student.UserProfile.fullName %>" style="width: 50px; height: 50px;"></td>
              <td><%= student.UserProfile.fullName %></td>
              <td><a href="/student-list/<%= student.id %>">Lihat Detail</a></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
</body>
</html>
